<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.OwnersBookingFragment">
   <androidx.constraintlayout.widget.ConstraintLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
      >

      <TextView
          android:id="@+id/topETxt"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="10dp"
          android:fontFamily="@font/poppins_bold"
          android:gravity="center"
          android:text="@string/bookedFragmetdetails"
          android:textSize="20sp"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent">

      </TextView>

      <androidx.recyclerview.widget.RecyclerView

          android:id="@+id/booiking_recycler"
          android:layout_width="match_parent"
          android:layout_height="0dp"
          android:layout_marginTop="10dp"
          app:layout_constraintBottom_toBottomOf="parent"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          android:layout_marginBottom="10dp"
          app:layout_constraintTop_toBottomOf="@+id/topETxt"
          tools:listitem="@layout/owners_room_booking_details" />


   </androidx.constraintlayout.widget.ConstraintLayout>




</FrameLayout>